{{ attach_library('charts/highcharts') }}
{{ attach_library('charts/generatechart') }}

<div id="container" style="width:100%; height:400px;">
    <h1>{{options.type}}</h1>
    <table>
        {% for row in data %}
            {% for col in row %}
                <tr><td>{{col.value}}</td><td>{{ col.label }}</td></tr>
            {% endfor %}
        {% endfor %}
    </table>

